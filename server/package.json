{
  "name": "SketchQuest-server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate": "ts-node -O \"{\\\"module\\\":\\\"commonjs\\\"}\" node_modules/knex/bin/cli.js migrate:latest --knexfile ./knexfile.ts",
    "seed": "ts-node -O \"{\\\"module\\\":\\\"commonjs\\\"}\" node_modules/knex/bin/cli.js seed:run --knexfile ./knexfile.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "knex": "^2.5.1",
    "mysql2": "^3.2.0",
    "socket.io": "^4.7.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.2",
    "@types/uuid": "^10.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  }
}
